<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Header"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <tab1
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Properties"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <logo
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldDescription="Configure Logo"
                                        fieldLabel="Logo"
                                        name="./logoImage"
                                        required="{Boolean}true"
                                        rootPath="/content/dam/us-bank-loayality"/>
                                    <logoalttext
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Configure Logo Alt Text"
                                        fieldLabel="Logo Alt Text"
                                        name="./logoAltText"
                                        required="{Boolean}true"/>
                                    <logourl
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldDescription="Configure Logo URL"
                                        fieldLabel="Logo URL"
                                        name="./logoUrl"
                                        required="{Boolean}true"
                                        rootPath="/content/us-bank-loyality"/>
                                    <newwindow
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        deleteHint="{Boolean}true"
                                        fieldDescription="If checked, link will be opened in new window"
                                        name="./newWindow"
                                        text="Open in New Window?"
                                        value="{Boolean}true"/>
                                    <account
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Configure Back to Account Label"
                                        fieldLabel="Back to account Label"
                                        name="./accountLabel"
                                        required="{Boolean}true"/>
                                    <accountLink
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldDescription="Configure Back Link URL"
                                        fieldLabel="Back Link URL"
                                        name="./accountLink"
                                        required="{Boolean}true"
                                        rootPath="/content/us-bank-loyality"/>
                                </items>
                            </column>
                        </items>
                    </tab1>
	                <primaryNavigation
	                        jcr:primaryType="nt:unstructured"
	                        jcr:title="PRIMARY NAVIGATION"
	                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
	                        margin="{Boolean}true">
	                        <items jcr:primaryType="nt:unstructured">
	                            <column
	                                jcr:primaryType="nt:unstructured"
	                                sling:resourceType="granite/ui/components/coral/foundation/container"
	                                margin="{Boolean}true">
	                                <items jcr:primaryType="nt:unstructured">
	                                    <navigationfields
	                                        jcr:primaryType="nt:unstructured"
	                                        sling:resourceType="/apps/us-bank-loyality/components/content/multifield"
	                                        fieldDescription="Click 'Add field' to add a new navigation field"
	                                        fieldLabel="Primary Navigation Fields">
	                                        <field
	                                            jcr:primaryType="nt:unstructured"
	                                            sling:resourceType="granite/ui/components/coral/foundation/container"
	                                            name="./primarynavfields">
	                                            <items jcr:primaryType="nt:unstructured">
	                                                <column
	                                                    jcr:primaryType="nt:unstructured"
	                                                    sling:resourceType="granite/ui/components/foundation/container">
	                                                    <items jcr:primaryType="nt:unstructured">
	                                                        <navigationLabel
	                                                            jcr:primaryType="nt:unstructured"
	                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
	                                                            emptyText="Navigation Label"
	                                                            fieldDescription="Configure Navigation Label"
	                                                            fieldLabel="Navigation Label"
                                        						required="{Boolean}true"
	                                                            name="./linkName"/>
	                                                        <navigationLink
	                                                            jcr:primaryType="nt:unstructured"
	                                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
	                                                            emptyText="Enter fully qualified URL for external URLs. Eg: https://www.gmfinancial.com"
	                                                            fieldDescription="Enter fully qualified URL for external URLs. Eg: https://www.gmfinancial.com"
	                                                            fieldLabel="Navigation Link"
	                                                            name="./linkUrl"
                                        						required="{Boolean}true"
	                                                            rootPath="/content/us-bank-loyality"/>
	                                                        <topNavNewWindow
	                                                            jcr:primaryType="nt:unstructured"
	                                                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
	                                                            fieldDescription="If Selected ‘True’ Link will be opened in new window"
	                                                            fieldLabel="New Window"
	                                                            name="./newWindow">
	                                                            <items jcr:primaryType="nt:unstructured">
	                                                                <false
	                                                                    jcr:primaryType="nt:unstructured"
	                                                                    text="False"
	                                                                    value="false"/>
	                                                                <true
	                                                                    jcr:primaryType="nt:unstructured"
	                                                                    text="True"
	                                                                    value="true"/>
	                                                            </items>
	                                                        </topNavNewWindow>
	                                                    </items>
	                                                </column>
	                                            </items>
	                                        </field>
	                                    </navigationfields>
	                                </items>
	                            </column>
	                        </items>
	                    </primaryNavigation>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
